class Static
{
int id;
String name;
static String college = "TRS";

public Static(int id, String name)
{
this.id =id;
this.name = name;
}

public void display()
{
System.out.println("your values of " + id +" " +name+ "the value of static variable is " + college);
}
public static void main(String []args)
{
Static st = new Static(141,"vinay");
System.out.println("displaying values");
st.display();
System.out.println("creating another object");
Static st1 = new Static(111,"hendi");
System.out.println("creating another objects and display its values");
st1.display();
}

}
